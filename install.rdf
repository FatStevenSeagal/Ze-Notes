    <em:localized>
      <Description>
        <em:locale>en-US</em:locale>
        <em:name>ZeNotes</em:name>
        <em:description>Advanced Notes Manager for Zotero</em:description>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>fr-FR</em:locale>
        <em:name>ZeNotes</em:name>
        <em:description>Module compl√©mentaire pour vos notes</em:description>
      </Description>
