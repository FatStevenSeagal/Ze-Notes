<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>zenotes@alefa.net</em:id>
    <em:name>ZeNotes</em:name>
    <em:version>0.3.2</em:version>
    <em:type>2</em:type>
    <em:multiprocessCompatible>true</em:multiprocessCompatible>
    <em:creator>Fanantenana Rianasoa Andriariniaina</em:creator>
    <em:homepageURL>https://github.com/frianasoa/zenotes</em:homepageURL>
    <em:optionsURL>chrome://zenotes/content/options.xul</em:optionsURL>
    <em:updateURL>https://raw.githubusercontent.com/frianasoa/zenotes/main/zenote-update.rdf</em:updateURL>
    <em:iconURL>chrome://zenotes/skin/icon.png</em:iconURL>
    <em:icon64URL>chrome://zenotes/skin/icon64.png</em:icon64URL>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>true</em:unpack>
    
    <em:targetApplication>
      <Description>
        <em:id>zotero@chnm.gmu.edu</em:id>
